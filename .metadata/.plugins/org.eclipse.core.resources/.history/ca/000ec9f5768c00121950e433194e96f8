package threedot7;

import java.util.LinkedList;

public class AnimalShelter {
	LinkedList ll = new LinkedList();
	

}

class Animal{
	String name;
	Animal(String name){
		this.name = name;
	}
	
}
 class Dog extends Animal{
	 Dog(String name){
		super(name);
	}
}
 
class Cat extends Animal{
	 Cat(String name){
		 super(name);
	 }
}
